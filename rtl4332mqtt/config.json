{
  "name": "RTL_433 to MQTT Bridge",
  "version": "0.6",
  "slug": "rtl4332mqtt",
  "description": "433MHz RF to MQTT Bridge based on RTL_SDR/RTL_433 for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/iceman73/hassio-addons/tree/master/rtl4332mqtt",
   "startup": "application",
  "boot": "auto",
  "map": ["config:rw", "ssl"],
  "usb": "True",
  "host_network": "False",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "options":
  {
    "mqtt_host": "hassio.local",
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "homeassistant/sensor/currentcost",
    "protocol": 3,
    "frequency": 433920000,
    "gain": 15,
    "frequency_offset": 0,
  "list_mqtttopic": "bedroom1:24,kitchen:128"
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "protocol": "int",
    "frequency": "int",
    "gain": "int",
    "frequency_offset": "int",
    "list_mqtttopic": "str"
   }
}
  
